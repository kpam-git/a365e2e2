{
    "compilerOptions": {
        "allowJs": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "declaration": false,
        "forceConsistentCasingInFileNames": true,
        "inlineSources": true,
        "jsx": "react",
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "sourceMap": true,
        "stripInternal": true,
        "target": "es5",
        "lib": [
            "es5",
            "es6",
            "es7",
            "dom"
        ]
    }
}